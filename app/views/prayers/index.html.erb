<div id="top-image">	<img src="/images/parable/parable_top_image.jpg" width="933" height="377" alt=""></div><div id="table-content" >	<h1>Listing prayers</h1>	<table width="600">  		<tr>    		<th width="15" height="38">First name</th>    		<th width="20">Last name</th>    		<th width="160">Prayer Request</th>    		<th width="10">Moderated</th>    		<th width="15" nowrap="nowrap"> Date Submitted</th>  		</tr>		<% @prayers.each do |prayer| %>  		<tr class="data">    		<td><%= prayer.first_name %></td>    		<td><%= prayer.last_name %></td>    		<td><%= prayer.request %></td>			<td><%= prayer.moderated %></td>			<td nowrap="nowrap"><%= prayer.created_at %></td>    		<td><%= link_to 'Show', prayer %></td>    		<td><%= link_to 'Moderate', edit_prayer_path(prayer) %></td>    		<td><%= link_to 'Delete', prayer, :confirm => 'Are you sure?', :method => :destroy %></td>  		</tr>		<% end %>	</table></div>